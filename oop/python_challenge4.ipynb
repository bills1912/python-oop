   "source": [
    "mph = float(input(\"What is your speed in miles per hour: \"))\n",
    "\n",
    "def mph_to_mps(num):\n",
    "    return f\"Your speed in meter per second is {num*0.44704}\"\n",
    "\n",
    "mph_to_mps(mph)"
   ]
